<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jxufe.ham.bean" table="taskAllot" catalog="houseagencymanagement" optimistic-lock="version">
		<id name ="taskAllotId" type ="int">
			<column name="taskAlloteID"/>
			<generator class="assigned"/>
		</id>
		<many-to-one name="employee" class="com.jxufe.ham.bean.Employee" fetch="select">
			<column name = "employeeID"></column>
		</many-to-one>
		<many-to-one name="task" class="com.jxufe.ham.bean" fetch="select">
			<column name="taskID"></column>
		</many-to-one>
	</class>
</hibernate-mapping>