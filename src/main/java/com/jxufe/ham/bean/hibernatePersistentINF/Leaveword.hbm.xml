<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jxufe.ham.bean.Leaveword" table="leaveword" dynamic-update="true" catalog="ham" optimistic-lock="version">
        <id name="leavewordId" type="int">
            <column name="leavewordID" />
            <generator class="assigned" />
        </id>
        <many-to-one name="employee" class="com.jxufe.ham.bean.Employee" >
            <column name="employeeID" not-null="true" />
        </many-to-one>
        <many-to-one name="leaveword" class="com.jxufe.ham.bean.Leaveword" >
            <column name="lea_leaveword" />
        </many-to-one>
        <property name="leavewordContent" type="string">
            <column name="leavewordContent" length="250" not-null="true" />
        </property>
        <property name="leavewordDate" type="date">
            <column name="leavewordDate" length="10" not-null="true" />
        </property>
        <property name="anonymousStatue" type="java.lang.Boolean">
            <column name="anonymousStatue" />
        </property>
        <set name="leavewords" table="leaveword" inverse="true" lazy="true" >
            <key>
                <column name="leavewordID" />
            </key>
            <one-to-many class="com.jxufe.ham.bean.Leaveword" />
        </set>
    </class>
</hibernate-mapping>
