<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jxufe.ham.bean.Authoritymanagement" table="authoritymanagement"
		dynamic-update="true" catalog="ham" optimistic-lock="version">
		<id name="authoritymanagementId" type="int">
			<column name="authoritymanagementID" />
			<generator class="assigned" />
		</id>
		<many-to-one name="role" class="com.jxufe.ham.bean.Role"
			fetch="select">
			<column name="roleID" />
		</many-to-one>
		<many-to-one name="authority" class="com.jxufe.ham.bean.Authority"
			fetch="select">
			<column name="authorityID" />
		</many-to-one>
	</class>
</hibernate-mapping>
