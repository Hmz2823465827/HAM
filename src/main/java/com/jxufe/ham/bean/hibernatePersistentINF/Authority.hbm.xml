<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jxufe.ham.bean.Authority" table="authority" dynamic-update="true" catalog="ham" optimistic-lock="version">
        <id name="authorityId" type="int">
            <column name="authorityID"/>
            <generator class="assigned" />
        </id>
        <property name="operation" type="string">
            <column name="operation" length="10"  />
        </property>
        <set name="authoritymanagements" table="authoritymanagement" inverse="true" lazy="true" >
            <key>
                <column name="authorityID" />
            </key>
            <one-to-many class="com.jxufe.ham.bean.Authoritymanagement" />
        </set>
    </class>
</hibernate-mapping>
